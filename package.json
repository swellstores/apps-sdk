{
  "name": "@swell/apps-sdk",
  "type": "module",
  "version": "1.0.39",
  "description": "Swell SDK for building isomorphic apps.",
  "author": "Swell",
  "license": "MIT",
  "engines": {
    "node": ">=14.19.1",
    "npm": ">=8.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swellstores/swell-apps-sdk"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/build.mjs",
    "postbuild": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm i && npm run build",
    "watch": "node scripts/build.mjs watch",
    "test": "jest"
  },
  "dependencies": {
    "@swell/easyblocks-core": "^1.0.10",
    "@types/lodash": "^4.17.5",
    "cache-manager": "^6.3.2",
    "color": "^4.2.3",
    "keyv": "^5.2.3",
    "liquidjs": "^10.16.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "qs": "^6.12.3",
    "quick-lru": "^7.0.0",
    "strftime": "^0.10.3",
    "swell-js": "^4.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@kruining/waterlogged": "^1.1.45",
    "@types/color": "^3.0.6",
    "@types/jest": "29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/strftime": "^0.9.8",
    "esbuild": "^0.24.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0"
  }
}
