{
  "name": "@swell/apps-sdk",
  "type": "module",
  "version": "1.0.147",
  "description": "Swell SDK for building isomorphic apps.",
  "author": "Swell",
  "license": "MIT",
  "engines": {
    "node": ">=18.16.1",
    "npm": ">=9.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swellstores/swell-apps-sdk"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/build.mjs",
    "postbuild": "tsc --project tsconfig.build.json",
    "prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'",
    "prepublishOnly": "npm i && npm run build",
    "watch": "node scripts/build.mjs watch",
    "test": "jest --silent"
  },
  "dependencies": {
    "@swell/easyblocks-core": "^1.0.19",
    "bluebird": "^3.7.2",
    "cache-manager": "^7.0.1",
    "color": "^4.2.3",
    "country-flag-icons": "1.5.18",
    "json5": "^2.2.3",
    "keyv": "^5.3.4",
    "liquidjs": "^10.21.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "qs": "^6.12.3",
    "strftime": "^0.10.3",
    "swell-js": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@jest/transform": "^29.7.0",
    "@types/bluebird": "^3.5.42",
    "@types/color": "^4.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/strftime": "^0.9.8",
    "esbuild": "^0.24.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
